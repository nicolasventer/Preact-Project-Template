{
	// "entryPoints": ["src/**/*.ts", "src/**/*.tsx"],
	"entryPoints": ["src/utils/**/*.*", "./tests/testUtils.ts"],
	"validation": {
		"notExported": true,
		"invalidLink": true,
		"notDocumented": true
	},
	"out": "typedoc_out",
	"logLevel": "Warn",
	"treatWarningsAsErrors": false,
	"excludeExternals": true,
	// "projectDocuments": ["README.md", "src/**/*.md"],
	"plugin": ["typedoc-plugin-missing-exports"],
	"exclude": ["src/**/*.d.ts"]
}
