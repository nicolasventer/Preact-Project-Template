{
	"entryPoints": ["src/**/*.ts", "src/**/*.tsx"],
	"validation": {
		"notExported": true,
		"invalidLink": true,
		"notDocumented": true
	},
	"out": "typedoc_out",
	"logLevel": "Warn",
	"treatWarningsAsErrors": false,
	"excludeExternals": true,
	// "projectDocuments": ["README.md", "src/**/*.md"],
	"projectDocuments": ["README.md"],
	"plugin": ["@zamiell/typedoc-plugin-not-exported"],
	"exclude": ["src/**/*.d.ts"]
}
