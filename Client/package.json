{
	"name": "client",
	"type": "module",
	"scripts": {
		"envDev": "bun run _genClientEnv.ts",
		"envProd": "cross-env B_PROD=true bun run _genClientEnv.ts",
		"dev": "bun run envDev && bunx --bun vite",
		"genHtml": "bun run ./node_modules/easy-react-router/build/plugin/_genRoutes.ts --html dist/index.html",
		"genApi": "bun run _genApi.ts",
		"buildDev": "bun run envDev && bun run _build.ts && bun run genHtml",
		"buildProd": "bun run envProd && bun run _build.ts && bun run genHtml",
		"build": "bun run buildDev",
		"preview": "serve -l 4173 ./dist",
		"previewProd": "bun run _previewProd.ts",
		"deploy": "bun run _deploy.ts",
		"buildDeploy": "bun run buildProd && bun run deploy",
		"devhost": "bunx --bun vite --host",
		"devhttps": "cross-env USE_HTTPS=true npx vite",
		"devhttpshost": "cross-env USE_HTTPS=true npx vite --host",
		"_build": "bunx --bun vite build # bun run build is preferred",
		"_preview": "vite preview",
		"doc": "typedoc && cmd /c \"start typedoc_out/index.html\"",
		"test": "bunx playwright test",
		"cov": "cross-env USE_COVERAGE=true bunx playwright test && cmd /c \"start coverage-reports/index.html\"",
		"testui": "bunx playwright test --ui",
		"testdebug": "bunx playwright test --debug",
		"testhtml": "cross-env USE_HTML=true bunx playwright test",
		"codegen": "bunx playwright codegen",
		"lint": "eslint .",
		"lintfix": "eslint --fix .",
		"depgraph": "depcruise --version && depcruise --config depcruise-config.cjs --output-type archi src | dot -T svg | depcruise-wrap-stream-in-html > graph-dependencies.html && cmd /c \"start graph-dependencies.html\""
	},
	"devDependencies": {
		"@eslint/js": "9.19.0",
		"@playwright/test": "1.50.1",
		"@preact/preset-vite": "2.10.1",
		"@types/bun": "1.2.2",
		"@types/dom-view-transitions": "1.0.5",
		"@types/node": "22.13.1",
		"@types/react": "19.0.8",
		"@types/react-dom": "19.0.3",
		"@typescript-eslint/eslint-plugin": "8.23.0",
		"@zamiell/typedoc-plugin-not-exported": "0.3.0",
		"cross-env": "7.0.3",
		"dependency-cruiser": "16.9.0",
		"eslint": "9.19.0",
		"eslint-plugin-no-relative-import-paths": "1.6.1",
		"eslint-plugin-project-structure": "3.13.2",
		"eslint-plugin-react": "7.37.4",
		"eslint-plugin-react-hooks": "5.1.0",
		"eslint-plugin-react-refresh": "0.4.18",
		"globals": "15.14.0",
		"jest": "29.7.0",
		"monocart-coverage-reports": "2.12.1",
		"monocart-reporter": "2.9.13",
		"ts-morph": "25.0.1",
		"tslib": "2.8.1",
		"typescript-eslint": "8.23.0",
		"vite-plugin-mkcert": "1.17.6"
	},
	"dependencies": {
		"@elysiajs/eden": "1.2.0",
		"@mantine/core": "7.16.2",
		"@mantine/hooks": "7.16.2",
		"@preact/signals": "2.0.0",
		"@sinclair/typebox": "0.34.16",
		"clsx": "2.1.1",
		"easy-react-router": "easy-react-router-1.0.0.tgz",
		"elysia": "1.2.12",
		"lucide-react": "0.474.0",
		"preact": "10.24.3",
		"react": "npm:@preact/compat",
		"react-dom": "npm:@preact/compat",
		"react-hot-toast": "2.5.1",
		"react-icons": "5.4.0"
	}
}
